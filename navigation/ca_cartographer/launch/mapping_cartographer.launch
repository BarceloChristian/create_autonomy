<launch>

  <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory $(find ca_cartographer)/config
          -configuration_basename ca_2d.lua"
      output="screen">
    <remap from="scan" to="create1/laser/scan" />
    <remap from="imu" to="create1/imu/data" />
    <remap from="odom" to="create1/odom" />
  </node>

  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
      type="cartographer_occupancy_grid_node" args="-resolution 0.05" />

  <!-- <param name="robot_description" textfile="$(find ca_cartographer)/urdf/create_2.urdf"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen">
    <param name="publish_frequency" type="double" value="10" />
    <param name="tf_prefix" value="create1_tf" />
  </node> -->

</launch>